<div class="form-container">
	<h1>Add Gallery</h1>
	<%= form_for @gallery, url: all_child_path(@user, @child) do |f| %>
	  <div class="input-field">
	    <%= f.text_field :gallery_name, placeholder: "Gallery Name" %>
	  </div>
	  <div class="float-fix">
		  <%= f.submit 'Add Gallery', class: 'button submit-button right' %>
		  <%= link_to 'Cancel', :back, class: 'button cancel-button right' %>
		</div>
	<% end %>
</div>
